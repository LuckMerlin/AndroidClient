<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="title"
            type="Object" />
        <variable
            name="message"
            type="Object" />
        <variable
            name="center"
            type="Object" />
        <variable
            name="left"
            type="Object" />
        <variable
            name="right"
            type="Object" />
        <variable
            name="content"
            type="Object" />
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</layout>
